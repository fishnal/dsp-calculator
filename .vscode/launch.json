	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch file",
			"type": "node",
			"request": "launch",
			"sourceMaps": true,
			"args": ["${file}"],
			"envFile": "${workspaceFolder}/.env",
			"runtimeArgs": ["--nolazy", "-r", "ts-node/register"],
			"cwd": "${workspaceFolder}",
			"console": "integratedTerminal"
		}
	]
}
